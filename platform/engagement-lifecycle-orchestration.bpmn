      <bpmndi:BPMNShape id="CallActivity_TOMAlignment_di" bpmnElement="CallActivity_TOMAlignment">
        <dc:Bounds x="837" y="324" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_GenerateReport_di" bpmnElement="Task_GenerateReport">
        <dc:Bounds x="335" y="534" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_Phase3Required_di" bpmnElement="Gateway_Phase3Required" isMarkerVisible="true">
        <dc:Bounds x="536" y="547" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="532" y="604" width="58" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="CallActivity_ContinuousMonitoring_di" bpmnElement="CallActivity_ContinuousMonitoring">
        <dc:Bounds x="1038" y="324" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_CloseEngagement_di" bpmnElement="Task_CloseEngagement">
        <dc:Bounds x="1038" y="109" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Closed_di" bpmnElement="EndEvent_Closed">
        <dc:Bounds x="1239" y="129" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1229" y="172" width="56" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Flow_01_di" bpmnElement="Flow_01">
        <di:waypoint x="283" y="147" />
        <di:waypoint x="335" y="147" />
      </bpmndi:BPMNEdge>

