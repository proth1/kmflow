      <bpmndi:BPMNShape id="CallActivity_EvidenceCollection_di" bpmnElement="CallActivity_EvidenceCollection">
        <dc:Bounds x="335" y="324" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="CallActivity_POVGeneration_di" bpmnElement="CallActivity_POVGeneration">
        <dc:Bounds x="536" y="324" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_Phase2Required_di" bpmnElement="Gateway_Phase2Required" isMarkerVisible="true">
        <dc:Bounds x="737" y="337" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="733" y="394" width="58" height="27" />
        </bpmndi:BPMNLabel>
