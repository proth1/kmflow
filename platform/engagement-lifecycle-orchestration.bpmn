      <bpmndi:BPMNShape id="Lane_DeliverableGeneration_di" bpmnElement="Lane_DeliverableGeneration" isHorizontal="true">
        <dc:Bounds x="185" y="467" width="1370" height="210" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="StartEvent_Created_di" bpmnElement="StartEvent_Created">
        <dc:Bounds x="247" y="129" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="237" y="172" width="56" height="27" />
