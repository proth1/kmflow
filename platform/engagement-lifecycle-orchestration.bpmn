<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  id="Definitions_Engagement"
  targetNamespace="http://bpmn.io/schema/bpmn"
  exporter="KMFlow Platform"
  exporterVersion="1.0.0">

  <bpmn:collaboration id="Collaboration_Engagement">
    <bpmn:participant id="Participant_Engagement" name="KMFlow Engagement Lifecycle" processRef="Process_EngagementLifecycle" />
  </bpmn:collaboration>

  <bpmn:process id="Process_EngagementLifecycle" isExecutable="true">
    <bpmn:laneSet id="LaneSet_Engagement">
      <bpmn:lane id="Lane_EngagementManagement" name="Engagement Management">
        <bpmn:flowNodeRef>StartEvent_Created</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ConfigureEngagement</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CloseEngagement</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Closed</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_CoreProcessing" name="Core Processing">
        <bpmn:flowNodeRef>CallActivity_EvidenceCollection</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CallActivity_POVGeneration</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Phase2Required</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CallActivity_TOMAlignment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CallActivity_ContinuousMonitoring</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_DeliverableGeneration" name="Deliverable Generation">
        <bpmn:flowNodeRef>Task_GenerateReport</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Phase3Required</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="StartEvent_Created" name="Engagement&#10;Created">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:userTask id="Task_ConfigureEngagement" name="Configure Engagement" camunda:candidateGroups="engagement-leads">
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_02</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:callActivity id="CallActivity_EvidenceCollection" name="Evidence&#10;Collection" calledElement="Process_EvidenceCollection">
      <bpmn:incoming>Flow_02</bpmn:incoming>
      <bpmn:outgoing>Flow_03</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:callActivity id="CallActivity_POVGeneration" name="POV&#10;Generation" calledElement="Process_POVGeneration">
      <bpmn:incoming>Flow_03</bpmn:incoming>
      <bpmn:outgoing>Flow_04</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:exclusiveGateway id="Gateway_Phase2Required" name="Phase 2&#10;Required?">
      <bpmn:incoming>Flow_04</bpmn:incoming>
      <bpmn:outgoing>Flow_05_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_05_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:callActivity id="CallActivity_TOMAlignment" name="TOM&#10;Alignment" calledElement="Process_TOMAlignment">
      <bpmn:incoming>Flow_05_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_06</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:serviceTask id="Task_GenerateReport" name="Generate Engagement Report" camunda:type="external" camunda:topic="kmflow.report.generate">
      <bpmn:incoming>Flow_05_No</bpmn:incoming>
      <bpmn:incoming>Flow_06</bpmn:incoming>
      <bpmn:outgoing>Flow_07</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_Phase3Required" name="Phase 3&#10;Required?">
      <bpmn:incoming>Flow_07</bpmn:incoming>
      <bpmn:outgoing>Flow_08_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_08_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:callActivity id="CallActivity_ContinuousMonitoring" name="Continuous&#10;Monitoring" calledElement="Process_ContinuousMonitoring">
      <bpmn:incoming>Flow_08_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_09</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:userTask id="Task_CloseEngagement" name="Close Engagement" camunda:candidateGroups="engagement-leads">
      <bpmn:incoming>Flow_08_No</bpmn:incoming>
      <bpmn:incoming>Flow_09</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="EndEvent_Closed" name="Engagement&#10;Closed">
      <bpmn:incoming>Flow_10</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_Created" targetRef="Task_ConfigureEngagement" />
    <bpmn:sequenceFlow id="Flow_02" sourceRef="Task_ConfigureEngagement" targetRef="CallActivity_EvidenceCollection" />
    <bpmn:sequenceFlow id="Flow_03" sourceRef="CallActivity_EvidenceCollection" targetRef="CallActivity_POVGeneration" />
    <bpmn:sequenceFlow id="Flow_04" sourceRef="CallActivity_POVGeneration" targetRef="Gateway_Phase2Required" />
    <bpmn:sequenceFlow id="Flow_05_Yes" name="Yes" sourceRef="Gateway_Phase2Required" targetRef="CallActivity_TOMAlignment" />
    <bpmn:sequenceFlow id="Flow_05_No" name="No" sourceRef="Gateway_Phase2Required" targetRef="Task_GenerateReport" />
    <bpmn:sequenceFlow id="Flow_06" sourceRef="CallActivity_TOMAlignment" targetRef="Task_GenerateReport" />
    <bpmn:sequenceFlow id="Flow_07" sourceRef="Task_GenerateReport" targetRef="Gateway_Phase3Required" />
    <bpmn:sequenceFlow id="Flow_08_Yes" name="Yes" sourceRef="Gateway_Phase3Required" targetRef="CallActivity_ContinuousMonitoring" />
    <bpmn:sequenceFlow id="Flow_08_No" name="No" sourceRef="Gateway_Phase3Required" targetRef="Task_CloseEngagement" />
    <bpmn:sequenceFlow id="Flow_09" sourceRef="CallActivity_ContinuousMonitoring" targetRef="Task_CloseEngagement" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_CloseEngagement" targetRef="EndEvent_Closed" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_Engagement">
    <bpmndi:BPMNPlane id="BPMNPlane_Engagement" bpmnElement="Collaboration_Engagement">
      <bpmndi:BPMNShape id="Participant_Engagement_di" bpmnElement="Participant_Engagement" isHorizontal="true">
        <dc:Bounds x="155" y="47" width="1400" height="630" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_EngagementManagement_di" bpmnElement="Lane_EngagementManagement" isHorizontal="true">
        <dc:Bounds x="185" y="47" width="1370" height="210" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_CoreProcessing_di" bpmnElement="Lane_CoreProcessing" isHorizontal="true">
        <dc:Bounds x="185" y="257" width="1370" height="210" />
