        <di:waypoint x="912" y="400" />
        <di:waypoint x="912" y="572" />
        <di:waypoint x="486" y="572" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_07_di" bpmnElement="Flow_07">
        <di:waypoint x="486" y="572" />
        <di:waypoint x="536" y="572" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_08_Yes_di" bpmnElement="Flow_08_Yes">
        <di:waypoint x="561" y="547" />
        <di:waypoint x="561" y="440" />
        <di:waypoint x="1113" y="440" />
