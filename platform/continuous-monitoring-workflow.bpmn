<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_Monitoring" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.42.0">
  <bpmn:process id="Process_ContinuousMonitoring" isExecutable="true">
    <bpmn:startEvent id="StartEvent_Activated" name="Monitoring&#10;Activated">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_Activated" targetRef="Task_ConfigureRules" />
    <bpmn:sequenceFlow id="Flow_02" sourceRef="Task_ConfigureRules" targetRef="Timer_DailyCollection" />
    <bpmn:sequenceFlow id="Flow_03" sourceRef="Timer_DailyCollection" targetRef="Task_CollectEvidence" />
    <bpmn:sequenceFlow id="Flow_04" sourceRef="Task_CollectEvidence" targetRef="Task_CompareBaseline" />
    <bpmn:sequenceFlow id="Flow_05" sourceRef="Task_CompareBaseline" targetRef="Gateway_DeviationDetected" />
    <bpmn:sequenceFlow id="Flow_06_No" name="No" sourceRef="Gateway_DeviationDetected" targetRef="Gateway_MergeBeforeControl" />
    <bpmn:sequenceFlow id="Flow_06_Yes" name="Yes" sourceRef="Gateway_DeviationDetected" targetRef="Gateway_SeverityLevel" />
    <bpmn:sequenceFlow id="Flow_07_Critical" name="Critical" sourceRef="Gateway_SeverityLevel" targetRef="Task_GenerateCriticalAlert" />
    <bpmn:sequenceFlow id="Flow_07_Low" name="Low" sourceRef="Gateway_SeverityLevel" targetRef="Task_AddToBatchReport" />
    <bpmn:sequenceFlow id="Flow_08" sourceRef="Task_GenerateCriticalAlert" targetRef="Task_ReviewCriticalDeviation" />
    <bpmn:sequenceFlow id="Flow_09" sourceRef="Task_AddToBatchReport" targetRef="Gateway_MergeBeforeControl" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_ReviewCriticalDeviation" targetRef="Gateway_ActionRequired" />
    <bpmn:sequenceFlow id="Flow_11_Regenerate" name="Regenerate POV" sourceRef="Gateway_ActionRequired" targetRef="CallActivity_TriggerPOV" />
    <bpmn:sequenceFlow id="Flow_11_Acknowledge" name="Acknowledge" sourceRef="Gateway_ActionRequired" targetRef="Gateway_MergeBeforeControl" />
    <bpmn:sequenceFlow id="Flow_12" sourceRef="CallActivity_TriggerPOV" targetRef="Gateway_MergeBeforeControl" />
    <bpmn:sequenceFlow id="Flow_MergeToControl" sourceRef="Gateway_MergeBeforeControl" targetRef="Gateway_MonitorControl" />
    <bpmn:sequenceFlow id="Flow_13_Continue" name="Continue" sourceRef="Gateway_MonitorControl" targetRef="Timer_DailyCollection" />
    <bpmn:sequenceFlow id="Flow_13_Stop" name="Stop" sourceRef="Gateway_MonitorControl" targetRef="EndEvent_Stopped">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${monitorAction == 'stop'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_13_Close" name="Close" sourceRef="Gateway_MonitorControl" targetRef="EndEvent_Complete">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${monitorAction == 'close'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="Gateway_DeviationDetected" name="Deviation&#10;Detected?">
      <bpmn:incoming>Flow_05</bpmn:incoming>
      <bpmn:outgoing>Flow_06_No</bpmn:outgoing>
      <bpmn:outgoing>Flow_06_Yes</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_ConfigureRules" name="Configure Monitoring Rules" camunda:type="external" camunda:topic="kmflow.monitor.configure">
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_02</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:intermediateCatchEvent id="Timer_DailyCollection" name="Daily&#10;Collection">
      <bpmn:incoming>Flow_02</bpmn:incoming>
      <bpmn:incoming>Flow_13_Continue</bpmn:incoming>
      <bpmn:outgoing>Flow_03</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">P1D</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:serviceTask id="Task_CollectEvidence" name="Collect Evidence Updates" camunda:type="external" camunda:topic="kmflow.monitor.collect">
      <bpmn:incoming>Flow_03</bpmn:incoming>
      <bpmn:outgoing>Flow_04</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_CompareBaseline" name="Compare Against Baseline" camunda:type="external" camunda:topic="kmflow.monitor.detect">
      <bpmn:incoming>Flow_04</bpmn:incoming>
      <bpmn:outgoing>Flow_05</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_SeverityLevel" name="Severity&#10;Level?">
      <bpmn:incoming>Flow_06_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_07_Critical</bpmn:outgoing>
      <bpmn:outgoing>Flow_07_Low</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_GenerateCriticalAlert" name="Generate Critical Alert" camunda:type="external" camunda:topic="kmflow.monitor.alert">
      <bpmn:incoming>Flow_07_Critical</bpmn:incoming>
      <bpmn:outgoing>Flow_08</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_AddToBatchReport" name="Add to Batch Report" camunda:type="external" camunda:topic="kmflow.monitor.report">
      <bpmn:incoming>Flow_07_Low</bpmn:incoming>
      <bpmn:outgoing>Flow_09</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_ReviewCriticalDeviation" name="Review Critical Deviation" camunda:candidateGroups="engagement-leads">
      <bpmn:incoming>Flow_08</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_ActionRequired" name="Action&#10;Required?">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11_Regenerate</bpmn:outgoing>
      <bpmn:outgoing>Flow_11_Acknowledge</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:callActivity id="CallActivity_TriggerPOV" name="Trigger POV Regeneration" calledElement="Process_POVGeneration">
      <bpmn:incoming>Flow_11_Regenerate</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:exclusiveGateway id="Gateway_MergeBeforeControl">
      <bpmn:incoming>Flow_06_No</bpmn:incoming>
      <bpmn:incoming>Flow_09</bpmn:incoming>
      <bpmn:incoming>Flow_11_Acknowledge</bpmn:incoming>
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_MergeToControl</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:exclusiveGateway id="Gateway_MonitorControl" name="Monitor&#10;Control ?" default="Flow_13_Continue">
      <bpmn:incoming>Flow_MergeToControl</bpmn:incoming>
      <bpmn:outgoing>Flow_13_Continue</bpmn:outgoing>
      <bpmn:outgoing>Flow_13_Stop</bpmn:outgoing>
      <bpmn:outgoing>Flow_13_Close</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_Stopped" name="Monitoring&#10;Stopped">
      <bpmn:incoming>Flow_13_Stop</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_Complete" name="Monitoring&#10;Complete">
      <bpmn:incoming>Flow_13_Close</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
