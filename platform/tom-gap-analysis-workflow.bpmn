<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  id="Definitions_TOMGapAnalysis"
  targetNamespace="http://bpmn.io/schema/bpmn"
  exporter="KMFlow Platform"
  exporterVersion="1.0.0">

  <bpmn:collaboration id="Collaboration_TOMGap">
    <bpmn:participant id="Participant_TOMGap" name="KMFlow TOM Gap Analysis" processRef="Process_TOMGapAnalysis" />
  </bpmn:collaboration>

  <bpmn:process id="Process_TOMGapAnalysis" isExecutable="true">
    <bpmn:laneSet id="LaneSet_TOMGap">
      <bpmn:lane id="Lane_TOMEngine" name="TOM Engine">
        <bpmn:flowNodeRef>StartEvent_TOMRequested</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_LoadTOM</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_LoadAsIs</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>SubProcess_Alignment</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_GapAnalysis" name="Gap Analysis Engine">
        <bpmn:flowNodeRef>Task_IdentifyGaps</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenerateRationale</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CalculatePriority</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CompileReport</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_EngagementLead" name="Engagement Lead">
        <bpmn:flowNodeRef>Task_ReviewGaps</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Approved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ProvideFeedback</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Complete</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="StartEvent_TOMRequested" name="TOM&#10;Analysis&#10;Requested">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:serviceTask id="Task_LoadTOM" name="Load Target Operating Model" camunda:type="external" camunda:topic="kmflow.tom.align">
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_02</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_LoadAsIs" name="Load As-Is Process Views" camunda:type="external" camunda:topic="kmflow.tom.align">
      <bpmn:incoming>Flow_02</bpmn:incoming>
      <bpmn:outgoing>Flow_03</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:subProcess id="SubProcess_Alignment" name="Multi-Dimensional&#10;Alignment">
      <bpmn:incoming>Flow_03</bpmn:incoming>
      <bpmn:outgoing>Flow_04</bpmn:outgoing>

      <bpmn:startEvent id="StartEvent_Align">
        <bpmn:outgoing>Flow_Sub01</bpmn:outgoing>
      </bpmn:startEvent>

      <bpmn:task id="Task_AlignPeople" name="Align People">
        <bpmn:incoming>Flow_Sub01</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub02</bpmn:outgoing>
      </bpmn:task>

      <bpmn:task id="Task_AlignProcess" name="Align Process">
        <bpmn:incoming>Flow_Sub02</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub03</bpmn:outgoing>
      </bpmn:task>

      <bpmn:task id="Task_AlignTechnology" name="Align Technology">
        <bpmn:incoming>Flow_Sub03</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub04</bpmn:outgoing>
      </bpmn:task>

      <bpmn:task id="Task_AlignData" name="Align Data">
        <bpmn:incoming>Flow_Sub04</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub05</bpmn:outgoing>
      </bpmn:task>

      <bpmn:task id="Task_AlignGovernance" name="Align Governance">
        <bpmn:incoming>Flow_Sub05</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub06</bpmn:outgoing>
      </bpmn:task>

      <bpmn:task id="Task_AlignOrganization" name="Align Organization">
        <bpmn:incoming>Flow_Sub06</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub07</bpmn:outgoing>
      </bpmn:task>

      <bpmn:endEvent id="EndEvent_Align">
        <bpmn:incoming>Flow_Sub07</bpmn:incoming>
      </bpmn:endEvent>

      <bpmn:sequenceFlow id="Flow_Sub01" sourceRef="StartEvent_Align" targetRef="Task_AlignPeople" />
      <bpmn:sequenceFlow id="Flow_Sub02" sourceRef="Task_AlignPeople" targetRef="Task_AlignProcess" />
      <bpmn:sequenceFlow id="Flow_Sub03" sourceRef="Task_AlignProcess" targetRef="Task_AlignTechnology" />
      <bpmn:sequenceFlow id="Flow_Sub04" sourceRef="Task_AlignTechnology" targetRef="Task_AlignData" />
      <bpmn:sequenceFlow id="Flow_Sub05" sourceRef="Task_AlignData" targetRef="Task_AlignGovernance" />
      <bpmn:sequenceFlow id="Flow_Sub06" sourceRef="Task_AlignGovernance" targetRef="Task_AlignOrganization" />
      <bpmn:sequenceFlow id="Flow_Sub07" sourceRef="Task_AlignOrganization" targetRef="EndEvent_Align" />
    </bpmn:subProcess>

    <bpmn:serviceTask id="Task_IdentifyGaps" name="Identify Gaps" camunda:type="external" camunda:topic="kmflow.tom.gaps">
      <bpmn:incoming>Flow_04</bpmn:incoming>
      <bpmn:incoming>Flow_Feedback</bpmn:incoming>
      <bpmn:outgoing>Flow_07</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_GenerateRationale" name="Generate LLM Rationale" camunda:type="external" camunda:topic="kmflow.llm.rationale">
      <bpmn:incoming>Flow_07</bpmn:incoming>
      <bpmn:outgoing>Flow_08</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_CalculatePriority" name="Calculate Gap Priority" camunda:type="external" camunda:topic="kmflow.tom.gaps">
      <bpmn:incoming>Flow_08</bpmn:incoming>
      <bpmn:outgoing>Flow_09</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_CompileReport" name="Compile Gap Report" camunda:type="external" camunda:topic="kmflow.tom.gaps">
      <bpmn:incoming>Flow_09</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_ReviewGaps" name="Review Gap Analysis" camunda:candidateGroups="engagement-leads">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:exclusiveGateway id="Gateway_Approved" name="Approved?">
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_12_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:userTask id="Task_ProvideFeedback" name="Provide Feedback" camunda:candidateGroups="engagement-leads">
      <bpmn:incoming>Flow_12_No</bpmn:incoming>
      <bpmn:outgoing>Flow_Feedback</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="EndEvent_Complete" name="Analysis&#10;Complete">
      <bpmn:incoming>Flow_12_Yes</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_TOMRequested" targetRef="Task_LoadTOM" />
    <bpmn:sequenceFlow id="Flow_02" sourceRef="Task_LoadTOM" targetRef="Task_LoadAsIs" />
    <bpmn:sequenceFlow id="Flow_03" sourceRef="Task_LoadAsIs" targetRef="SubProcess_Alignment" />
    <bpmn:sequenceFlow id="Flow_04" sourceRef="SubProcess_Alignment" targetRef="Task_IdentifyGaps" />
    <bpmn:sequenceFlow id="Flow_07" sourceRef="Task_IdentifyGaps" targetRef="Task_GenerateRationale" />
    <bpmn:sequenceFlow id="Flow_08" sourceRef="Task_GenerateRationale" targetRef="Task_CalculatePriority" />
    <bpmn:sequenceFlow id="Flow_09" sourceRef="Task_CalculatePriority" targetRef="Task_CompileReport" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_CompileReport" targetRef="Task_ReviewGaps" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_ReviewGaps" targetRef="Gateway_Approved" />
    <bpmn:sequenceFlow id="Flow_12_Yes" name="Yes" sourceRef="Gateway_Approved" targetRef="EndEvent_Complete" />
    <bpmn:sequenceFlow id="Flow_12_No" name="No" sourceRef="Gateway_Approved" targetRef="Task_ProvideFeedback" />
    <bpmn:sequenceFlow id="Flow_Feedback" sourceRef="Task_ProvideFeedback" targetRef="Task_IdentifyGaps" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_TOMGap">
