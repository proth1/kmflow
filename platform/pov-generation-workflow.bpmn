<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  id="Definitions_1"
  targetNamespace="http://bpmn.io/schema/bpmn"
  exporter="KMFlow Platform"
  exporterVersion="1.0.0">

  <bpmn:collaboration id="Collaboration_POV">
    <bpmn:participant id="Participant_POV" name="KMFlow POV Generation" processRef="Process_POV" />
  </bpmn:collaboration>

  <bpmn:process id="Process_POV" isExecutable="true">

    <!-- Lane: POV Engine -->
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_POVEngine" name="POV Engine">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_InitializeContext</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_ConfidenceLevel</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AssemblePOV</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenerateBPMN</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Generated</bpmn:flowNodeRef>
      </bpmn:lane>

      <!-- Lane: Semantic Engine -->
      <bpmn:lane id="Lane_SemanticEngine" name="Semantic Engine">
        <bpmn:flowNodeRef>Subprocess_LCDAlgorithm</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ExtractEntities</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_BuildGraph</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Subprocess_GraphTraversal</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_TriangulateSources</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Contradictions</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_BuildConsensus</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CalculateConfidence</bpmn:flowNodeRef>
      </bpmn:lane>

      <!-- Lane: Process Analyst -->
      <bpmn:lane id="Lane_ProcessAnalyst" name="Process Analyst">
        <bpmn:flowNodeRef>Task_ResolveContradictions</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EscalateToLead</bpmn:flowNodeRef>
      </bpmn:lane>

      <!-- Lane: SME -->
      <bpmn:lane id="Lane_SME" name="SME">
        <bpmn:flowNodeRef>Task_SMEReview</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="POV&#10;Requested">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- POV Engine Lane -->
    <bpmn:serviceTask id="Task_InitializeContext" name="Initialize POV Context" camunda:type="external" camunda:topic="kmflow.pov.consensus">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_ConfidenceLevel" name="Confidence&#10;Level?">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_HighConfidence</bpmn:outgoing>
      <bpmn:outgoing>Flow_LowConfidence</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:serviceTask id="Task_AssemblePOV" name="Assemble POV Model" camunda:type="external" camunda:topic="kmflow.pov.assemble">
      <bpmn:incoming>Flow_HighConfidence</bpmn:incoming>
      <bpmn:incoming>Flow_FromSME</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_GenerateBPMN" name="Generate BPMN Output" camunda:type="external" camunda:topic="kmflow.pov.bpmn">
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="EndEvent_Generated" name="POV&#10;Generated">
      <bpmn:incoming>Flow_16</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Semantic Engine Lane -->
    <bpmn:subProcess id="Subprocess_LCDAlgorithm" name="LCD Algorithm Core">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>

      <bpmn:startEvent id="Start_LCD">
        <bpmn:outgoing>Flow_LCD1</bpmn:outgoing>
      </bpmn:startEvent>
      <bpmn:serviceTask id="Task_LCD_ExtractEntities" name="Extract Entities" camunda:type="external" camunda:topic="kmflow.semantic.extract">
        <bpmn:incoming>Flow_LCD1</bpmn:incoming>
        <bpmn:outgoing>Flow_LCD2</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:serviceTask id="Task_LCD_BuildRelationships" name="Build Relationships" camunda:type="external" camunda:topic="kmflow.semantic.relate">
        <bpmn:incoming>Flow_LCD2</bpmn:incoming>
        <bpmn:outgoing>Flow_LCD3</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:serviceTask id="Task_LCD_IdentifyPatterns" name="Identify Patterns" camunda:type="external" camunda:topic="kmflow.semantic.patterns">
        <bpmn:incoming>Flow_LCD3</bpmn:incoming>
        <bpmn:outgoing>Flow_LCD4</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:endEvent id="End_LCD">
        <bpmn:incoming>Flow_LCD4</bpmn:incoming>
      </bpmn:endEvent>

      <bpmn:sequenceFlow id="Flow_LCD1" sourceRef="Start_LCD" targetRef="Task_LCD_ExtractEntities" />
      <bpmn:sequenceFlow id="Flow_LCD2" sourceRef="Task_LCD_ExtractEntities" targetRef="Task_LCD_BuildRelationships" />
      <bpmn:sequenceFlow id="Flow_LCD3" sourceRef="Task_LCD_BuildRelationships" targetRef="Task_LCD_IdentifyPatterns" />
      <bpmn:sequenceFlow id="Flow_LCD4" sourceRef="Task_LCD_IdentifyPatterns" targetRef="End_LCD" />
    </bpmn:subProcess>

    <bpmn:serviceTask id="Task_ExtractEntities" name="Extract Process Entities" camunda:type="external" camunda:topic="kmflow.semantic.extract">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_BuildGraph" name="Build Semantic Graph" camunda:type="external" camunda:topic="kmflow.semantic.triangulate">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:subProcess id="Subprocess_GraphTraversal" name="Evidence Graph Traversal">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>

      <bpmn:startEvent id="Start_GraphTraversal">
        <bpmn:outgoing>Flow_GT1</bpmn:outgoing>
      </bpmn:startEvent>
      <bpmn:serviceTask id="Task_TraverseSources" name="Traverse Sources" camunda:type="external" camunda:topic="kmflow.semantic.traverse">
        <bpmn:incoming>Flow_GT1</bpmn:incoming>
        <bpmn:outgoing>Flow_GT2</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:serviceTask id="Task_CrossReference" name="Cross-Reference" camunda:type="external" camunda:topic="kmflow.semantic.crossref">
        <bpmn:incoming>Flow_GT2</bpmn:incoming>
        <bpmn:outgoing>Flow_GT3</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:serviceTask id="Task_ScoreConfidence" name="Score Confidence" camunda:type="external" camunda:topic="kmflow.semantic.confidence">
        <bpmn:incoming>Flow_GT3</bpmn:incoming>
        <bpmn:outgoing>Flow_GT4</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:endEvent id="End_GraphTraversal">
        <bpmn:incoming>Flow_GT4</bpmn:incoming>
      </bpmn:endEvent>

      <bpmn:sequenceFlow id="Flow_GT1" sourceRef="Start_GraphTraversal" targetRef="Task_TraverseSources" />
      <bpmn:sequenceFlow id="Flow_GT2" sourceRef="Task_TraverseSources" targetRef="Task_CrossReference" />
      <bpmn:sequenceFlow id="Flow_GT3" sourceRef="Task_CrossReference" targetRef="Task_ScoreConfidence" />
      <bpmn:sequenceFlow id="Flow_GT4" sourceRef="Task_ScoreConfidence" targetRef="End_GraphTraversal" />
    </bpmn:subProcess>

    <bpmn:serviceTask id="Task_TriangulateSources" name="Triangulate Sources" camunda:type="external" camunda:topic="kmflow.semantic.triangulate">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_Contradictions" name="Contradictions&#10;Detected?">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_NoContradictions</bpmn:outgoing>
      <bpmn:outgoing>Flow_YesContradictions</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:serviceTask id="Task_BuildConsensus" name="Build Consensus Model" camunda:type="external" camunda:topic="kmflow.pov.consensus">
      <bpmn:incoming>Flow_NoContradictions</bpmn:incoming>
      <bpmn:incoming>Flow_FromAnalyst</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_CalculateConfidence" name="Calculate Confidence Score" camunda:type="external" camunda:topic="kmflow.pov.confidence">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- Process Analyst Lane -->
    <bpmn:userTask id="Task_ResolveContradictions" name="Resolve Contradictions" camunda:candidateGroups="process-analysts">
      <bpmn:incoming>Flow_YesContradictions</bpmn:incoming>
      <bpmn:outgoing>Flow_FromAnalyst</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_EscalateToLead" name="Escalate to Lead Analyst" camunda:candidateGroups="process-analysts">
      <bpmn:incoming>Flow_TimerEscalate</bpmn:incoming>
    </bpmn:userTask>

    <!-- SME Lane -->
    <bpmn:userTask id="Task_SMEReview" name="SME Review &amp; Validate" camunda:candidateGroups="smes">
      <bpmn:incoming>Flow_LowConfidence</bpmn:incoming>
      <bpmn:outgoing>Flow_FromSME</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:boundaryEvent id="BoundaryEvent_SMETimer" name="48 Hours" cancelActivity="false" attachedToRef="Task_SMEReview">
      <bpmn:outgoing>Flow_TimerEscalate</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT48H</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_InitializeContext" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_InitializeContext" targetRef="Subprocess_LCDAlgorithm" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Subprocess_LCDAlgorithm" targetRef="Task_ExtractEntities" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_ExtractEntities" targetRef="Task_BuildGraph" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_BuildGraph" targetRef="Subprocess_GraphTraversal" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Subprocess_GraphTraversal" targetRef="Task_TriangulateSources" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_TriangulateSources" targetRef="Gateway_Contradictions" />
    <bpmn:sequenceFlow id="Flow_NoContradictions" name="No" sourceRef="Gateway_Contradictions" targetRef="Task_BuildConsensus">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${contradictions == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_YesContradictions" name="Yes" sourceRef="Gateway_Contradictions" targetRef="Task_ResolveContradictions">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${contradictions == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_FromAnalyst" sourceRef="Task_ResolveContradictions" targetRef="Task_BuildConsensus" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_BuildConsensus" targetRef="Task_CalculateConfidence" />
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Task_CalculateConfidence" targetRef="Gateway_ConfidenceLevel" />
    <bpmn:sequenceFlow id="Flow_HighConfidence" name="Confidence >= 0.5" sourceRef="Gateway_ConfidenceLevel" targetRef="Task_AssemblePOV">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${confidenceScore >= 0.5}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_LowConfidence" name="Confidence &lt; 0.5" sourceRef="Gateway_ConfidenceLevel" targetRef="Task_SMEReview">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${confidenceScore &lt; 0.5}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_FromSME" sourceRef="Task_SMEReview" targetRef="Task_AssemblePOV" />
    <bpmn:sequenceFlow id="Flow_15" sourceRef="Task_AssemblePOV" targetRef="Task_GenerateBPMN" />
    <bpmn:sequenceFlow id="Flow_16" sourceRef="Task_GenerateBPMN" targetRef="EndEvent_Generated" />
    <bpmn:sequenceFlow id="Flow_TimerEscalate" sourceRef="BoundaryEvent_SMETimer" targetRef="Task_EscalateToLead" />

  </bpmn:process>

  <!-- BPMN Diagram -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_POV">

      <!-- Pool -->
      <bpmndi:BPMNShape id="Participant_POV_di" bpmnElement="Participant_POV" isHorizontal="true">
        <dc:Bounds x="155" y="47" width="1705" height="793" />
      </bpmndi:BPMNShape>

      <!-- Lanes -->
      <bpmndi:BPMNShape id="Lane_POVEngine_di" bpmnElement="Lane_POVEngine" isHorizontal="true">
        <dc:Bounds x="185" y="47" width="1675" height="198" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_SemanticEngine_di" bpmnElement="Lane_SemanticEngine" isHorizontal="true">
        <dc:Bounds x="185" y="245" width="1675" height="198" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_ProcessAnalyst_di" bpmnElement="Lane_ProcessAnalyst" isHorizontal="true">
        <dc:Bounds x="185" y="443" width="1675" height="198" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_SME_di" bpmnElement="Lane_SME" isHorizontal="true">
        <dc:Bounds x="185" y="641" width="1675" height="152" />
      </bpmndi:BPMNShape>

      <!-- POV Engine Lane Elements -->
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="247" y="128" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="242" y="171" width="46" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_InitializeContext_di" bpmnElement="Task_InitializeContext">
        <dc:Bounds x="335" y="108" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_ConfidenceLevel_di" bpmnElement="Gateway_ConfidenceLevel">
        <dc:Bounds x="1317" y="121" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1311" y="178" width="62" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AssemblePOV_di" bpmnElement="Task_AssemblePOV">
        <dc:Bounds x="1477" y="108" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_GenerateBPMN_di" bpmnElement="Task_GenerateBPMN">
        <dc:Bounds x="1678" y="108" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Generated_di" bpmnElement="EndEvent_Generated">
        <dc:Bounds x="1735" y="228" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1779" y="238" width="46" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Semantic Engine Lane Elements -->
      <bpmndi:BPMNShape id="Subprocess_LCDAlgorithm_di" bpmnElement="Subprocess_LCDAlgorithm" isExpanded="false">
        <dc:Bounds x="335" y="304" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ExtractEntities_di" bpmnElement="Task_ExtractEntities">
        <dc:Bounds x="485" y="306" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_BuildGraph_di" bpmnElement="Task_BuildGraph">
        <dc:Bounds x="686" y="306" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Subprocess_GraphTraversal_di" bpmnElement="Subprocess_GraphTraversal" isExpanded="false">
        <dc:Bounds x="887" y="304" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_TriangulateSources_di" bpmnElement="Task_TriangulateSources">
        <dc:Bounds x="1037" y="306" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_Contradictions_di" bpmnElement="Gateway_Contradictions">
        <dc:Bounds x="1238" y="319" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1226" y="376" width="74" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_BuildConsensus_di" bpmnElement="Task_BuildConsensus">
        <dc:Bounds x="1416" y="306" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_CalculateConfidence_di" bpmnElement="Task_CalculateConfidence">
        <dc:Bounds x="1617" y="306" width="151" height="76" />
      </bpmndi:BPMNShape>

      <!-- Process Analyst Lane Elements -->
      <bpmndi:BPMNShape id="Task_ResolveContradictions_di" bpmnElement="Task_ResolveContradictions">
        <dc:Bounds x="1238" y="504" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_EscalateToLead_di" bpmnElement="Task_EscalateToLead">
        <dc:Bounds x="1477" y="504" width="151" height="76" />
      </bpmndi:BPMNShape>

      <!-- SME Lane Elements -->
      <bpmndi:BPMNShape id="Task_SMEReview_di" bpmnElement="Task_SMEReview">
        <dc:Bounds x="1267" y="679" width="151" height="76" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="BoundaryEvent_SMETimer_di" bpmnElement="BoundaryEvent_SMETimer">
        <dc:Bounds x="1400" y="737" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1438" y="759" width="48" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Sequence Flow Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="283" y="146" />
        <di:waypoint x="335" y="146" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="410" y="184" />
        <di:waypoint x="410" y="344" />
        <di:waypoint x="435" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="435" y="344" />
        <di:waypoint x="485" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="636" y="344" />
        <di:waypoint x="686" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="837" y="344" />
        <di:waypoint x="887" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="987" y="344" />
        <di:waypoint x="1037" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_7_di" bpmnElement="Flow_7">
        <di:waypoint x="1188" y="344" />
        <di:waypoint x="1238" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_NoContradictions_di" bpmnElement="Flow_NoContradictions">
        <di:waypoint x="1288" y="344" />
        <di:waypoint x="1416" y="344" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1345" y="326" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_YesContradictions_di" bpmnElement="Flow_YesContradictions">
        <di:waypoint x="1263" y="369" />
        <di:waypoint x="1263" y="542" />
        <di:waypoint x="1238" y="542" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1269" y="403" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_FromAnalyst_di" bpmnElement="Flow_FromAnalyst">
        <di:waypoint x="1389" y="542" />
        <di:waypoint x="1491" y="542" />
        <di:waypoint x="1491" y="382" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_10_di" bpmnElement="Flow_10">
        <di:waypoint x="1567" y="344" />
        <di:waypoint x="1617" y="344" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_12_di" bpmnElement="Flow_12">
        <di:waypoint x="1692" y="306" />
        <di:waypoint x="1692" y="146" />
        <di:waypoint x="1367" y="146" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_HighConfidence_di" bpmnElement="Flow_HighConfidence">
        <di:waypoint x="1367" y="146" />
        <di:waypoint x="1477" y="146" />
        <bpmndi:BPMNLabel>
