<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_IncomeVerification" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.42.0">
  <bpmn:process id="Process_IncomeVerification" name="Income Verification" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Income&#10;Verification&#10;Initiated">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_GatherDocs" name="Gather Income Documentation" camunda:candidateGroups="processors">
      <bpmn:documentation>Evidence: EV-016, EV-018, EV-019</bpmn:documentation>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_EmploymentType" name="Employment&#10;Type?">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_W2</bpmn:outgoing>
      <bpmn:outgoing>Flow_SelfEmployed</bpmn:outgoing>
      <bpmn:outgoing>Flow_Other</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_VerifyW2" name="Verify W-2 &#38; Pay Stubs" camunda:candidateGroups="processors">
      <bpmn:documentation>Evidence: EV-016</bpmn:documentation>
      <bpmn:incoming>Flow_W2</bpmn:incoming>
      <bpmn:outgoing>Flow_W2_Complete</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_AnalyzeTaxReturns" name="Analyze Tax Returns &#38; P&#38;L" camunda:candidateGroups="processors">
      <bpmn:documentation>Evidence: EV-018</bpmn:documentation>
      <bpmn:incoming>Flow_SelfEmployed</bpmn:incoming>
      <bpmn:outgoing>Flow_SE_Complete</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_VerifyOtherIncome" name="Verify Other Income Sources" camunda:candidateGroups="processors">
      <bpmn:documentation>Evidence: EV-019, EV-001</bpmn:documentation>
      <bpmn:incoming>Flow_Other</bpmn:incoming>
      <bpmn:outgoing>Flow_Other_Complete</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_MergeEmployment">
      <bpmn:incoming>Flow_W2_Complete</bpmn:incoming>
      <bpmn:incoming>Flow_SE_Complete</bpmn:incoming>
      <bpmn:incoming>Flow_Other_Complete</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_Calculate" name="Calculate Qualifying Income" camunda:type="external" camunda:topic="income-calculation">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_ReviewCalculation" name="Review Income Calculation" camunda:candidateGroups="underwriters">
      <bpmn:documentation>Evidence: EV-001, EV-002</bpmn:documentation>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_IncomeTrending" name="Income&#10;Stable?">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_Stable</bpmn:outgoing>
      <bpmn:outgoing>Flow_Declining</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_DocumentDeclining" name="Document Declining Income Rationale" camunda:candidateGroups="underwriters">
      <bpmn:incoming>Flow_Declining</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_Verified" name="Income&#10;Verified">
      <bpmn:incoming>Flow_Stable</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_ConditionalVerified" name="Income&#10;Verified&#10;(Conditional)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_GatherDocs" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_GatherDocs" targetRef="Gateway_EmploymentType" />
    <bpmn:sequenceFlow id="Flow_W2" name="W-2 Employee" sourceRef="Gateway_EmploymentType" targetRef="Task_VerifyW2">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${employmentType == 'W2'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_SelfEmployed" name="Self-Employed" sourceRef="Gateway_EmploymentType" targetRef="Task_AnalyzeTaxReturns">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${employmentType == 'SelfEmployed'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Other" name="Other Income" sourceRef="Gateway_EmploymentType" targetRef="Task_VerifyOtherIncome">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${employmentType == 'Other'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_W2_Complete" sourceRef="Task_VerifyW2" targetRef="Gateway_MergeEmployment" />
    <bpmn:sequenceFlow id="Flow_SE_Complete" sourceRef="Task_AnalyzeTaxReturns" targetRef="Gateway_MergeEmployment" />
    <bpmn:sequenceFlow id="Flow_Other_Complete" sourceRef="Task_VerifyOtherIncome" targetRef="Gateway_MergeEmployment" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Gateway_MergeEmployment" targetRef="Task_Calculate" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_Calculate" targetRef="Task_ReviewCalculation" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_ReviewCalculation" targetRef="Gateway_IncomeTrending" />
    <bpmn:sequenceFlow id="Flow_Stable" name="Yes" sourceRef="Gateway_IncomeTrending" targetRef="EndEvent_Verified">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${incomeTrend == 'Stable'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Declining" name="No" sourceRef="Gateway_IncomeTrending" targetRef="Task_DocumentDeclining">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${incomeTrend == 'Declining'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_DocumentDeclining" targetRef="EndEvent_ConditionalVerified" />
  </bpmn:process>
