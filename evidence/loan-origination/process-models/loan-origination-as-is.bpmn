      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_RunAUS_di" bpmnElement="Task_RunAUS">
        <dc:Bounds x="572" y="150" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_DeliverLE_di" bpmnElement="Task_DeliverLE">
        <dc:Bounds x="724" y="150" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ObtainIntent_di" bpmnElement="Task_ObtainIntent">
        <dc:Bounds x="876" y="150" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_IntentToProceed_di" bpmnElement="Gateway_IntentToProceed" isMarkerVisible="true">
        <dc:Bounds x="1028" y="165" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1012" y="133" width="82" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_NoIntent_di" bpmnElement="End_NoIntent">
        <dc:Bounds x="1130" y="172" width="36" height="36" />
