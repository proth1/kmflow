      </bpmndi:BPMNShape>

      <!-- ═══ Row 2: Processing Phase (y=400) ═══ -->

      <bpmndi:BPMNShape id="Task_SetupFile_di" bpmnElement="Task_SetupFile">
        <dc:Bounds x="180" y="400" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_OrderServices_di" bpmnElement="Task_OrderServices">
        <dc:Bounds x="332" y="400" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_MergeCollect_di" bpmnElement="Gateway_MergeCollect" isMarkerVisible="true">
        <dc:Bounds x="484" y="415" width="50" height="50" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CollectDocs_di" bpmnElement="Task_CollectDocs">
        <dc:Bounds x="586" y="400" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ValidateDocs_di" bpmnElement="Task_ValidateDocs">
        <dc:Bounds x="738" y="400" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_DocsComplete_di" bpmnElement="Gateway_DocsComplete" isMarkerVisible="true">
        <dc:Bounds x="890" y="415" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="884" y="383" width="62" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_SubmitToUW_di" bpmnElement="Task_SubmitToUW">
        <dc:Bounds x="992" y="400" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- ═══ Row 3: Underwriting Phase (y=650) ═══ -->

      <bpmndi:BPMNShape id="Gateway_MergeReview_di" bpmnElement="Gateway_MergeReview" isMarkerVisible="true">
        <dc:Bounds x="180" y="665" width="50" height="50" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ReviewFile_di" bpmnElement="Task_ReviewFile">
        <dc:Bounds x="282" y="650" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_AnalyzeCredit_di" bpmnElement="Task_AnalyzeCredit">
        <dc:Bounds x="434" y="650" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_VerifyIncome_di" bpmnElement="Task_VerifyIncome">
        <dc:Bounds x="586" y="650" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ReviewAppraisal_di" bpmnElement="Task_ReviewAppraisal">
        <dc:Bounds x="738" y="650" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_UWDecision_di" bpmnElement="Gateway_UWDecision" isMarkerVisible="true">
        <dc:Bounds x="890" y="665" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="888" y="633" width="54" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_IssueConditions_di" bpmnElement="Task_IssueConditions">
        <dc:Bounds x="992" y="650" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Deny branch -->
      <bpmndi:BPMNShape id="Task_DenyLoan_di" bpmnElement="Task_DenyLoan">
        <dc:Bounds x="992" y="810" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_Denied_di" bpmnElement="End_Denied">
        <dc:Bounds x="1144" y="832" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1132" y="873" width="60" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Suspend branch -->
      <bpmndi:BPMNShape id="Task_SuspendLoan_di" bpmnElement="Task_SuspendLoan">
        <dc:Bounds x="992" y="940" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Row 3: Condition Clearing -->
      <bpmndi:BPMNShape id="Gateway_MergeClear_di" bpmnElement="Gateway_MergeClear" isMarkerVisible="true">
        <dc:Bounds x="1144" y="665" width="50" height="50" />
      </bpmndi:BPMNShape>
