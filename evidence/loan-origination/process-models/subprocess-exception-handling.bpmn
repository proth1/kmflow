      <!-- SVP Denied End -->
      <bpmndi:BPMNShape id="EndEvent_SVPDenied_di" bpmnElement="EndEvent_SVPDenied">
        <dc:Bounds x="1544" y="292" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1530" y="328" width="64" height="40" />
        </bpmndi:BPMNLabel>
