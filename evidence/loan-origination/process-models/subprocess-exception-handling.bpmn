      <bpmndi:BPMNShape id="EndEvent_CCOApproved_di" bpmnElement="EndEvent_CCOApproved">
        <dc:Bounds x="1888" y="72" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1874" y="108" width="64" height="40" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- CCO Denied End -->
      <bpmndi:BPMNShape id="EndEvent_CCODenied_di" bpmnElement="EndEvent_CCODenied">
        <dc:Bounds x="1888" y="292" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1874" y="328" width="64" height="40" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- FICO Fork Gateway (below main flow) -->
      <bpmndi:BPMNShape id="Gateway_FICOFork_di" bpmnElement="Gateway_FICOFork">
        <dc:Bounds x="620" y="425" width="50" height="50" />
      </bpmndi:BPMNShape>

      <!-- FICO Primary Review -->
      <bpmndi:BPMNShape id="Task_FICOPrimary_di" bpmnElement="Task_FICOPrimary">
        <dc:Bounds x="740" y="370" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- FICO Secondary Review -->
      <bpmndi:BPMNShape id="Task_FICOSecondary_di" bpmnElement="Task_FICOSecondary">
        <dc:Bounds x="740" y="470" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- FICO Join Gateway -->
      <bpmndi:BPMNShape id="Gateway_FICOJoin_di" bpmnElement="Gateway_FICOJoin">
        <dc:Bounds x="900" y="425" width="50" height="50" />
      </bpmndi:BPMNShape>

      <!-- FICO Both Approved Gateway -->
      <bpmndi:BPMNShape id="Gateway_FICOBothApproved_di" bpmnElement="Gateway_FICOBothApproved" isMarkerVisible="true">
        <dc:Bounds x="1002" y="425" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="975" y="398" width="56" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- FICO Approved End -->
      <bpmndi:BPMNShape id="EndEvent_FICOApproved_di" bpmnElement="EndEvent_FICOApproved">
        <dc:Bounds x="1104" y="432" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1090" y="468" width="76" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- FICO Denied End -->
      <bpmndi:BPMNShape id="EndEvent_FICODenied_di" bpmnElement="EndEvent_FICODenied">
        <dc:Bounds x="1104" y="502" width="36" height="36" />
