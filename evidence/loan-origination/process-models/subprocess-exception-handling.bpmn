      </bpmndi:BPMNShape>

      <!-- LTV Task (fan-out center) -->
      <bpmndi:BPMNShape id="Task_PrepareLTV_di" bpmnElement="Task_PrepareLTV">
        <dc:Bounds x="600" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- General Task (fan-out bottom) -->
      <bpmndi:BPMNShape id="Task_PrepareGeneral_di" bpmnElement="Task_PrepareGeneral">
        <dc:Bounds x="600" y="270" width="100" height="80" />
