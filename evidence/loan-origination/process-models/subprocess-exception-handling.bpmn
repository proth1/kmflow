      </bpmndi:BPMNShape>

      <!-- Timer Boundary Event (bottom-center of ReviewException) -->
      <bpmndi:BPMNShape id="BoundaryEvent_Timer_di" bpmnElement="BoundaryEvent_Timer">
        <dc:Bounds x="996" y="222" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="990" y="260" width="52" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Manager Decision Gateway -->
      <bpmndi:BPMNShape id="Gateway_ManagerDecision_di" bpmnElement="Gateway_ManagerDecision" isMarkerVisible="true">
        <dc:Bounds x="1116" y="175" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1060" y="145" width="62" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Manager Approved End -->
      <bpmndi:BPMNShape id="EndEvent_ManagerApproved_di" bpmnElement="EndEvent_ManagerApproved">
        <dc:Bounds x="1200" y="72" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1186" y="108" width="64" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Manager Denied End -->
      <bpmndi:BPMNShape id="EndEvent_ManagerDenied_di" bpmnElement="EndEvent_ManagerDenied">
        <dc:Bounds x="1200" y="292" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1186" y="328" width="64" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Review Escalated Task -->
      <bpmndi:BPMNShape id="Task_ReviewEscalated_di" bpmnElement="Task_ReviewEscalated">
        <dc:Bounds x="1290" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- SVP Decision Gateway -->
      <bpmndi:BPMNShape id="Gateway_SVPDecision_di" bpmnElement="Gateway_SVPDecision" isMarkerVisible="true">
        <dc:Bounds x="1442" y="175" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1394" y="145" width="42" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- SVP Approved End -->
      <bpmndi:BPMNShape id="EndEvent_SVPApproved_di" bpmnElement="EndEvent_SVPApproved">
        <dc:Bounds x="1544" y="72" width="36" height="36" />
